{
  "name":"qiannw/my-framework",
  "description":"a test framework",
  "authors":[
    {
      "name":"qiannw",
      "email":"nengwuqian@gmail.com"
    }
  ],
  "config":{
    "secure-http": false
  },
  "require":{
    "php":">=7.0.0",
    "slim/slim":"^3.0"
  },
  "require-dev":{
    "phpunit/phpunit":"~6.0"
  },
  "autoload":{
    "classmap":[
      "app/Models"
    ],
    "psr-4":{
      "App\\":"app/"
    },
    "files":[
      "app/helpers.php"
    ]
  },
  "autoload-dev":{
    "psr-4":{
      "Test\\":"test"
    }
  },
  "repositories":[
    {
      "type":"composer",
      "url":"https://packagist.phpcomposer.com"
    }
  ]
}